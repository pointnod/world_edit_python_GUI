      {
        "name": "//wand",
        "description": "Obtenir l'outil de sélection (hache en bois par défaut)",
        "syntax": "//wand",
        "params": []
      },
      {
        "name": "//pos1",
        "description": "Définir la première position de sélection à votre emplacement actuel",
        "syntax": "//pos1",
        "params": []
      },
      {
        "name": "//pos2",
        "description": "Définir la deuxième position de sélection à votre emplacement actuel",
        "syntax": "//pos2",
        "params": []
      },
      {
        "name": "//hpos1",
        "description": "Définir la première position sur le bloc visé",
        "syntax": "//hpos1",
        "params": []
      },
      {
        "name": "//hpos2",
        "description": "Définir la deuxième position sur le bloc visé",
        "syntax": "//hpos2",
        "params": []
