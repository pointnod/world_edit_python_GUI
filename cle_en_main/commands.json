        "description": "Changer le type de sélection (cuboid, extend, poly, ellipsoid, sphere, cyl, convex, cylinder, line, polyhedron)",
        "syntax": "//sel [type]",
        "params": [
          {
            "name": "type",
            "type": "choice",
            "choices": ["cuboid", "extend", "poly", "ellipsoid", "sphere", "cyl", "convex", "cylinder", "line", "polyhedron"],
            "default": "cuboid",
            "required": false
          }
        ]
      },
      {
        "name": "//chunk",
        "description": "Sélectionner le chunk entier où vous vous trouvez",
        "syntax": "//chunk",
        "params": []
